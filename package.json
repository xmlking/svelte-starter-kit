		"prettier-plugin-svelte": "2.10.0",
		"prettier-plugin-tailwindcss": "0.2.7",
		"svelecte": "3.16.0",
		"svelte": "3.58.0",
		"svelte-check": "3.2.0",
		"svelte-headless-table": "0.17.3",
		"svelte-heros-v2": "0.4.2",
		"svelte-time-distance": "0.0.3",
		"sveltekit-superforms": "0.8.0",
		"tailwindcss": "3.3.1",
		"tslib": "2.5.0",
		"typescript": "5.0.4",
		"vite": "4.3.1",
		"vite-plugin-pwa": "0.14.7",
		"vitest": "0.30.1",
