{
	"name": "svelte-starter-kit",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"build:lib": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"preview": "NEXTAUTH_URL=http://localhost:4173 AUTH_TRUST_HOST=true vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@auth/core": "0.7.0",
		"@auth/sveltekit": "0.3.1",
		"@faker-js/faker": "7.6.0",
		"@felte/reporter-svelte": "1.1.5",
		"@felte/validator-zod": "1.0.13",
		"@neoconfetti/svelte": "1.0.0",
		"@neodrag/svelte": "2.0.3",
		"@popperjs/core": "2.11.7",
		"@sentry/svelte": "7.49.0",
		"@sentry/tracing": "7.49.0",
		"@skeletonlabs/skeleton": "1.2.0",
		"@svelte-put/resize": "2.0.0",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/adapter-node": "1.2.3",
		"@sveltejs/kit": "1.15.7",
		"@sveltejs/package": "2.0.2",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/line-clamp": "0.4.4",
		"@tailwindcss/typography": "0.5.9",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@types/gtag.js": "0.0.12",
		"@types/js-cookie": "3.0.3",
		"@types/testing-library__jest-dom": "5.14.5",
		"@typescript-eslint/eslint-plugin": "5.59.0",
		"@typescript-eslint/parser": "5.59.0",
		"@vite-pwa/sveltekit": "0.2.1",
		"@vitest/coverage-c8": "0.30.1",
		"autoprefixer": "10.4.14",
		"c8": "7.13.0",
		"clsx": "1.2.1",
		"cross-fetch": "3.1.5",
		"cssnano": "6.0.0",
		"d3-force": "3.0.0",
		"d3-scale": "4.0.2",
		"daisyui": "2.51.5",
		"date-fns": "2.29.3",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"felte": "1.2.7",
		"flowbite": "1.6.5",
		"flowbite-svelte": "0.34.9",
		"graphql": "16.6.0",
