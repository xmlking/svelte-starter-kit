{
	"name": "svelte-starter-kit",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:debug": "DEBUG=* vite dev",
		"dev:sw": "SW_DEV=true vite dev",
		"build": "vite build",
		"build:lib": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"preview": "NEXTAUTH_URL=http://localhost:4173 vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@auth/core": "0.8.2",
		"@auth/sveltekit": "0.3.1",
		"@felte/reporter-svelte": "1.1.5",
		"@felte/validator-zod": "1.0.13",
		"@neoconfetti/svelte": "1.0.0",
		"@neodrag/svelte": "2.0.3",
		"@popperjs/core": "2.11.8",
		"@skeletonlabs/skeleton": "1.7.1",
		"@svelte-put/resize": "2.0.0",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-node": "1.2.4",
		"@sveltejs/kit": "1.20.2",
		"@sveltejs/package": "2.0.2",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@types/gtag.js": "0.0.12",
		"@types/js-cookie": "3.0.3",
		"@types/testing-library__jest-dom": "5.14.6",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"@vite-pwa/sveltekit": "0.2.2",
		"@vitest/coverage-c8": "0.32.0",
		"autoprefixer": "10.4.14",
		"c8": "7.14.0",
		"clsx": "1.2.1",
		"cross-fetch": "3.1.6",
		"cssnano": "6.0.1",
		"d3-force": "3.0.0",
		"d3-scale": "4.0.2",
		"daisyui": "3.0.20",
		"date-fns": "2.30.0",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"felte": "1.2.7",
		"flowbite": "1.6.5",
		"flowbite-svelte": "0.37.4",
		"graphql": "16.6.0",
		"graphql-ws": "5.13.1",
		"houdini": "1.2.5",
		"houdini-svelte": "1.2.5",
		"jose": "4.14.4",
		"js-cookie": "3.0.5",
		"jsdom": "22.1.0",
		"layercake": "7.4.0",
		"msw": "1.2.1",
		"postcss": "8.4.24",
		"postcss-preset-env": "8.4.2",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-headless-table": "0.17.3",
		"svelte-heros-v2": "0.7.1",
		"svelte-select": "5.6.1",
		"svelte-time-distance": "0.0.3",
		"sveltekit-flash-message": "1.0.0-rc.3",
		"sveltekit-superforms": "1.0.0-rc.3",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-plugin-pwa": "0.16.3",
		"vitest": "0.32.0",
		"workbox-window": "7.0.0",
		"zod": "3.21.4"
	},
	"optionalDependencies": {
		"@playwright/test": "1.35.0",
		"@vitest/ui": "0.32.0"
	},
	"packageManager": "pnpm@8.6.0",
	"engines": {
		"pnpm": ">=8"
	},
	"type": "module",
	"dependencies": {
		"cookie": "0.5.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xmlking/svelte-starter-kit"
	}
}
